# 4.11
lamb = 1
pdf1 <- function(x) {
r = 1
(lamb * r) ^ r / factorial(r - 1) * exp(1) ^ (- lamb * r * x) * x ^ (r-1)
}
pdf2 <- function(x) {
r = 2
(lamb * r) ^ r / factorial(r - 1) * exp(1) ^ (- lamb * r * x) * x ^ (r-1)
}
pdf4 <- function(x) {
r = 4
(lamb * r) ^ r / factorial(r - 1) * exp(1) ^ (- lamb * r * x) * x ^ (r-1)
}
pdf8 <- function(x) {
r = 8
(lamb * r) ^ r / factorial(r - 1) * exp(1) ^ (- lamb * r * x) * x ^ (r-1)
}
pdf16 <- function(x) {
r = 16
(lamb * r) ^ r / factorial(r - 1) * exp(1) ^ (- lamb * r * x) * x ^ (r-1)
}
